    sh:property [ sh:class <http://example.org/Acquisition> ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path <http://example.org/Acquisition> ],
        [ sh:class <http://example.org/OSCEMSample> ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path <http://example.org/oscemsample> ],
        [ sh:class <http://example.org/Instrument> ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path <http://example.org/Instrument> ] ;
    sh:targetClass oscem_schemas:EMDataset .

<http://example.org/Author> a sh:NodeShape ;
    sh:closed true ;
    sh:description "A class representing an author" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "Author" ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Email address of the author" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "Email" ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path <http://example.org/email> ],
        [ sh:class <http://example.org/Organization> ;
            sh:description "The organization the author is affiliated with" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "Organization" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path <http://example.org/organization> ],
        [ sh:datatype xsd:string ;
            sh:description "Role of the author, i.e., principal investigator" ;
            sh:maxCount 1 ;
            sh:name "Role" ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path <http://example.org/role> ],
        [ sh:datatype xsd:string ;
            sh:description "Phone number of the author" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "Phone" ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path <http://example.org/phone> ],
        [ sh:datatype xsd:string ;
            sh:description "ORCID of the author, a type of unique identifier" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "ORCID" ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path <http://example.org/orcid> ],
        [ sh:class <http://example.org/AuthorName> ;
            sh:description "Full name of the author" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "Name" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path <http://example.org/author_name> ],
        [ sh:datatype xsd:string ;
            sh:description "Country of the author's institution" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "Country" ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path <http://example.org/country> ] ;
    sh:targetClass <http://example.org/Author> .

