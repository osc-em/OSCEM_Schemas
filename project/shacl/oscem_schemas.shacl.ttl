            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "Name" ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path <http://example.org/name_mol> ],
        [ sh:datatype xsd:string ;
            sh:description "Full sequence of the sample as in the data, i.e., cleaved tags should also be removed from sequence here" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "Sequence" ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path <http://example.org/sequence> ],
        [ sh:datatype xsd:string ;
            sh:description "Scientific name of the organism used to produce the molecule of interest" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "Expression_system" ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path <http://example.org/expression_system> ],
        [ sh:datatype xsd:string ;
            sh:description "Scientific name of the natural host organism" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "Natural_source" ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path <http://example.org/natural_source> ],
        [ sh:datatype xsd:string ;
