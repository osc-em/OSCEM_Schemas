            sh:maxCount 1 ;
            sh:name "Pretreatment_type" ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path <http://example.org/pretreatment_type> ],
        [ sh:datatype xsd:string ;
            sh:description "Grid manufacturer" ;
            sh:maxCount 1 ;
            sh:name "Manufacturer" ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path <http://example.org/manufacturer> ],
        [ sh:datatype xsd:boolean ;
            sh:description "Whether a support film was used" ;
            sh:maxCount 1 ;
            sh:name "Film_support" ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path <http://example.org/film_support> ],
        [ sh:datatype xsd:float ;
            sh:description "Length of time of the pretreatment in s" ;
            sh:maxCount 1 ;
            sh:name "Pretreatment_time" ;
            sh:nodeKind sh:Literal ;
            sh:order 8 ;
