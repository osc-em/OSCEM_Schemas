<https://w3id.org/osc-em/instrument/:Instrument> a sh:NodeShape ;
    sh:closed true ;
    sh:description "Instrument values, mostly constant across a data collection." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Mode of imaging used during data collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path <https://w3id.org/osc-em/instrument/:imaging> ],
        [ sh:class <http://qudt.org/schema/qudtquantityValue> ;
            sh:description "C2 aperture size used in data acquisition, in µm" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 5 ;
            sh:path <https://w3id.org/osc-em/instrument/:c2_aperture> ],
        [ sh:class <http://qudt.org/schema/qudtquantityValue> ;
            sh:description "Voltage used for the electron acceleration, in kV" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path <https://w3id.org/osc-em/instrument/:acceleration_voltage> ],
        [ sh:datatype xsd:string ;
