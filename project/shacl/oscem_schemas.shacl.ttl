            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path <https://w3id.org/osc-em/acquisitionspherical_aberration_corrector> ] ;
    sh:targetClass <https://w3id.org/osc-em/acquisitionSpecialistOptics> .

<https://w3id.org/osc-em/acquisitionSphericalAberrationCorrector> a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:boolean ;
            sh:description "whether a specific instrument was used during data acquisition" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path <https://w3id.org/osc-em/acquisitionused> ],
        [ sh:datatype xsd:string ;
            sh:description "Details of a given specialist instrument" ;
