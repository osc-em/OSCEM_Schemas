            sh:path <https://w3id.org/osc-em/instrument/:electron_source> ],
        [ sh:class <http://qudt.org/schema/qudt/quantityValue> ;
            sh:description "Spherical aberration of the instrument, in mm" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
