<https://w3id.org/osc-em/cellular_tomoGrowthCondition> a sh:NodeShape ;
    sh:closed true ;
    sh:description "how the cells were grown" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "What was the targeted cell cycle state, if any" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path <https://w3id.org/osc-em/cellular_tomocell_cycle> ],
        [ sh:datatype xsd:string ;
            sh:description "What was the atmosphere the sample was in, elaborate on any special gases present etc." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path <https://w3id.org/osc-em/cellular_tomoatmosphere_growth> ],
        [ sh:datatype xsd:string ;
            sh:description "In/on what kind of surface/container the cells were grown; i.e. directly on a grid" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path <https://w3id.org/osc-em/cellular_tomogrowth_location> ],
        [ sh:class <https://w3id.org/osc-em/custom_typesQuantitySI> ;
            sh:description "temperature of the sample; in K." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 5 ;
            sh:path <https://w3id.org/osc-em/cellular_tomotemperature_growth> ],
        [ sh:datatype xsd:string ;
            sh:description "were there any special treatment conditions; such as addition of certain chemicals etc" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path <https://w3id.org/osc-em/cellular_tomotreatment> ],
        [ sh:datatype xsd:string ;
            sh:description "What growth media was used" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path <https://w3id.org/osc-em/cellular_tomomedia> ] ;
    sh:targetClass <https://w3id.org/osc-em/cellular_tomoGrowthCondition> .

