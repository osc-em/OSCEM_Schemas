<https://w3id.org/osc-em/custom_typesSeries> a sh:NodeShape ;
    rdfs:subClassOf <https://w3id.org/osc-em/custom_typesRange> ;
    sh:closed true ;
    sh:description "A series of numbers constructed from min, max, and increment" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class <https://w3id.org/osc-em/custom_typesQuantitySI> ;
            sh:description "Minimal value of a given dataset property" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
