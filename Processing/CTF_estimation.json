{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://schemas.com/processing/CTF",
    "title": "CTF_estimation",
    "description": "JSON Schema for electron microscopy CTF estimation part of processing",
    "type": "object",
    "properties": {
        "Defocus":{
            "type": "object",
            "properties": {
                "Output_min_defocus":{
                    "description": "Minimum defocus",
                    "type": "number"
                },
                "Output_max_defocus":{
                    "description": "Maximum defocus",
                    "type": "number"
                },
                "Ouput_avg_defocus":{
                    "description": "Average value of defocus",
                    "type": "number"
                },
                "Defocus_histogram":{
                    "description": "Name of histogram of decofus values",
                    "type": "string"
                }
            },
            "additionalProperties": false
        },
        "Resolution":{
            "type": "object",
            "properties": {
                "Output_min_resolution":{
                    "description": "Minimum resolution",
                    "type": "number"
                },
                "Output_max_resolution":{
                    "description": "Maximum resolution",
                    "type": "number"
                },
                "Ouput_avg_resolution":{
                    "description": "Average value of resolution",
                    "type": "number"
                },
                "Resolution_histogram":{
                    "description": "Name of histogram of resolution values",
                    "type": "string"
                }
            },
            "additionalProperties": false
        },
        "Astigmatism":{
            "type": "object",
            "properties": {
                "Astigmatism_histogram":{
                    "description": "Name of histogram of astigmatism values",
                    "type": "string"
                }
            }
        }
    },
    "additionalProperties": false
}
