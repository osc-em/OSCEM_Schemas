{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://schemas.com/processing/movie_maxshift",
    "title": "movie_maxshift",
    "description": "JSON Schema for electron microscopy movie max shift part of processing",
    "type": "object",
    "properties": {
        "Discarded_movies": {
            "description": "Number of discarded movies",
            "type": "number"
        },
        "Rejection_type": {
            "description": "Rejection type for discarding images",
            "type": "string"
        },
        "Max_frame_shift_(A)": {
            "description": "Maximum frame shift to discard images",
            "type": "number"
        },
        "Max_movie_shift_(A)": {
            "description": "Maximum movie shift to discard images",
            "type": "number"
        },
        "Ouput_avg_shift_(A)": {
            "description": "Average shift at output",
            "type": "number"
        },
        "Output_max_shift_(A)": {
            "description": "Maximum shift at output",
            "type": "number"
        },
        "Max_shift_histogram": {
            "description": "Filename of the shift values histogram"
        }
    },
    "additionalProperties": false
}