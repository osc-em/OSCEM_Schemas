{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://schemas.com/processing/import_movies",
    "title": "import_movies",
    "description": "JSON Schema for electron microscopy import movies part of processing",
    "type": "object",
    "properties": {
        "Number_movies": {
            "description": "Number of movies imported",
            "type": "number"
        },
        "Frames_per_movie": {
            "description": "Number of frames per movie",
            "type": "number"
        },
        "Frames_size": {
            "description": "Size of the frames",
            "type": "object",
            "properties": {
                "value": {
                    "type": "string",
                    "description": "Frame sizes value"
                },
                "unit": {
                    "type": "string",
                    "description": "Unit of frame size"
                }
            },
            "required": ["value", "unit"]
        },
        "Dose_per_image":{
            "description": "Dose per image",
            "type": "object",
            "properties": {
                "value": {
                    "type": "number",
                    "description": "Dose per image value"
                },
                "unit": {
                    "type": "string",
                    "description": "Unit of dose per image"
                }
            },
            "required": ["value", "unit"]
        },
        "Initial_dose":{
            "description": "Initial dose",
            "type": "object",
            "properties": {
                "value": {
                    "type": "number",
                    "description": "Initial dose value"
                },
                "unit": {
                    "type": "string",
                    "description": "Unit of initial dose"
                }
            },
            "required": ["value", "unit"]
        },
        "Microscope_voltage": {
            "description": "Microscope voltage",
            "type": "object",
            "properties": {
                "value": {
                    "type": "number",
                    "description": "Voltage value"
                },
                "unit": {
                    "type": "string",
                    "description": "Unit of voltage"
                }
            },
            "required": ["value", "unit"]
        },
        "Spherical_aberration": {
            "description": "Spherical aberration of microscope",
            "type": "object",
            "properties": {
                "value": {
                    "type": "number",
                    "description": "Spherical aberration value"
                },
                "unit": {
                    "type": "string",
                    "description": "Unit of spherical aberration"
                }
            },
            "required": ["value", "unit"]
        },
        "Pixel_size": {
            "description": "Pixel size",
            "type": "object",
            "properties": {
                "value": {
                    "type": "number",
                    "description": "Pixel size value"
                },
                "unit": {
                    "type": "string",
                    "description": "Unit of pixel size"
                }
            },
            "required": ["value", "unit"]
        },
        "Gain_image": {
            "description": "Gain image filename",
            "type": "string"
        },
        "Dark_image": {
            "description": "Dark image filename",
            "type": "string"
        }
    },
    "additionalProperties": false
}