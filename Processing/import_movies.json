{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://schemas.com/processing/import_movies",
    "title": "import_movies",
    "description": "JSON Schema for electron microscopy import movies part of processing",
    "type": "object",
    "properties": {
        "Number_movies": {
            "description": "Number of movies imported",
            "type": "number"
        },       
        "Dose_per_image (e/A²)":{
            "description": "Dose per image given in electrons per square Angstrom",
            "type": "number"
        },
        "Initial_dose (e/A²)":{
            "description": "Initial dose in electrons per square Angstrom",
            "type": "number"
        },
        "Microscope_voltage (kV)": {
            "description": "Microscope voltage in kV",
            "type": "number"
        },
        "Spherical_aberration (mm)": {
            "description": "Spherical aberration of microscope in mm",
            "type": "number"
        },
        "Amplitud_contrast": {
            "description": "Amplitud contrast",
            "type": "number"
        },
        "Pixel_size (Å/px)": {
            "description": "Pixel size",
            "type": "number"
        },
        "Gain_image": {
            "description": "Gain image filename",
            "type": "string"
        },
        "Dark_image": {
            "description": "Dark image filename",
            "type": "string"
        }
    },
    "additionalProperties": false
}