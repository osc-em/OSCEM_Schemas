{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://schemas.com/processing/import_movies",
    "title": "import_movies",
    "description": "JSON Schema for electron microscopy import movies part of processing",
    "type": "object",
    "properties": {
        "Dose_per_image":{
            "description": "Dose per image",
            "type": "object",
            "properties": {
                "value": {
                    "type": "number",
                    "description": "Dose per image value"
                },
                "unit": {
                    "type": "string",
                    "description": "Unit of dose per image"
                }
            },
            "required": ["value", "unit"]
        },
        "Initial_dose":{
            "description": "Initial dose",
            "type": "object",
            "properties": {
                "value": {
                    "type": "number",
                    "description": "Initial dose value"
                },
                "unit": {
                    "type": "string",
                    "description": "Unit of initial dose"
                }
            },
            "required": ["value", "unit"]
        },
        "Gain_image": {
            "description": "Gain image filename",
            "type": "string"
        },
        "Dark_image": {
            "description": "Dark image filename",
            "type": "string"
        }
    },
    "additionalProperties": false
}