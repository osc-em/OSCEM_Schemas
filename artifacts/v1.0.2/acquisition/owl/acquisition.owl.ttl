acquisition:beamshift a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "beamshift" ;
    rdfs:range types:BoundingBox2D ;
    skos:definition "Movement of the beam above the sample for data collection purposes that does not require movement of the stage. Given in mrad." ;
    skos:inScheme acquisition: .

acquisition:beamtilt a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "beamtilt" ;
    rdfs:range types:BoundingBox2D ;
    skos:definition "Another way to move the beam above the sample for data collection purposes that does not require movement of the stage. Given in mrad." ;
    skos:inScheme acquisition: .

acquisition:beamtiltgroups a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "beamtiltgroups" ;
    rdfs:range linkml:Integer ;
    skos:definition "Number of Beamtilt groups present in this dataset - for optimized processing split dataset into groups of same tilt angle. Despite its name Beamshift is often used to achive this result." ;
    skos:inScheme acquisition: .

acquisition:binning_camera a owl:ObjectProperty,
