    skos:inScheme acquisition: ;
    linkml:permissible_values <https://w3id.org/oscem-schemas/latest/acquisitionAcquisitionTechnique#cellular_tomo>,
        <https://w3id.org/oscem-schemas/latest/acquisitionAcquisitionTechnique#env_tomo>,
        <https://w3id.org/oscem-schemas/latest/acquisitionAcquisitionTechnique#spa>,
