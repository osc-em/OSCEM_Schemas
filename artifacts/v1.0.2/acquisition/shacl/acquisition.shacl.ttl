@prefix acquisition: <https://w3id.org/oscem-schemas/latest/acquisition> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix types: <https://w3id.org/oscem-schemas/latest/custom_types> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

acquisition:Acquisition a sh:NodeShape ;
    sh:closed true ;
    sh:description "General acquisition covering materials science and other use cases. For specialized techniques, use the appropriate subclass (AcquisitionSpa for single particle, or tomography subclasses)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:integer ;
            sh:description "Number of Beamtilt groups present in this dataset - for optimized processing split dataset into groups of same tilt angle. Despite its name Beamshift is often used to achive this result." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 26 ;
            sh:path acquisition:beamtiltgroups ],
        [ sh:datatype xsd:integer ;
            sh:description "Magnification level as indicated by the instrument, no unit" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path acquisition:nominal_magnification ],
        [ sh:datatype xsd:integer ;
            sh:description "Number of grids imaged for this project - here with qualifier during this data acquisition" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 18 ;
            sh:path acquisition:grids_imaged ],
        [ sh:class types:QuantitySI ;
            sh:description "Pixel size, in Angstrom" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 21 ;
            sh:path acquisition:pixel_size ],
        [ sh:class types:Range ;
            sh:description "Target defocus set, min and max values in nm." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path acquisition:nominal_defocus ],
        [ sh:class types:ImageSize ;
            sh:description "The size of the image in pixels, height and width given." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 13 ;
            sh:path acquisition:image_size ],
        [ sh:datatype xsd:integer ;
            sh:description "Calculated magnification, no unit" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path acquisition:calibrated_magnification ],
        [ sh:class types:BoundingBox2D ;
            sh:description "Movement of the Beam below the image in order to shift the image on the detector. Given in µm." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 25 ;
            sh:path acquisition:imageshift ],
        [ sh:class types:Range ;
            sh:description "Temperature during data collection, in K with min and max values." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 8 ;
            sh:path acquisition:temperature_range ],
        [ sh:class types:BoundingBox2D ;
            sh:description "Movement of the beam above the sample for data collection purposes that does not require movement of the stage. Given in mrad." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 23 ;
            sh:path acquisition:beamshift ],
        [ sh:class acquisition:SpecialistOptics ;
            sh:description "Any type of special optics, such as a phaseplate" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 22 ;
            sh:path acquisition:specialist_optics ],
        [ sh:datatype xsd:string ;
            sh:description "Type of cryogen used in the holder - if the holder is cooled seperately" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path acquisition:holder_cryogen ],
        [ sh:datatype xsd:string ;
            sh:description "Speciman holder model" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path acquisition:holder ],
        [ sh:datatype xsd:dateTime ;
            sh:description "Time and date of the data acquisition" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 14 ;
            sh:path acquisition:date_time ],
        [ sh:class acquisition:EnergyFilter ;
            sh:description "Whether an energy filter was used and its specifics." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 12 ;
            sh:path acquisition:energy_filter ],
        [ sh:class types:ImageSize ;
            sh:description "Level of binning on the images applied during data collection" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 20 ;
            sh:path acquisition:binning_camera ],
        [ sh:class types:BoundingBox2D ;
            sh:description "Another way to move the beam above the sample for data collection purposes that does not require movement of the stage. Given in mrad." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 24 ;
            sh:path acquisition:beamtilt ],
        [ sh:datatype xsd:integer ;
            sh:description "Number of frames that on average constitute a full movie, can be a bit hard to define for some detectors" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 17 ;
            sh:path acquisition:frames_per_movie ],
        [ sh:datatype xsd:string ;
            sh:description "Whether and how you have to flip or rotate the gainref in order to align with your acquired images" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 27 ;
            sh:path acquisition:gainref_flip_rotate ],
        [ sh:class types:QuantitySI ;
            sh:description "Average dose per image/movie/tilt - given in electrons per square Angstrom" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 11 ;
            sh:path acquisition:dose_per_movie ],
        [ sh:datatype xsd:integer ;
            sh:description "Number of images generated total for this data collection - might need a qualifier for tilt series to determine whether full series or individual tilts are counted" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 19 ;
            sh:path acquisition:images_generated ],
        [ sh:class types:Range ;
            sh:description "Machine estimated defocus, min and max values in µm. Has a tendency to be off." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path acquisition:calibrated_defocus ],
        [ sh:class types:QuantitySI ;
            sh:description "Time of data acquisition per movie/tilt - in s" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 15 ;
            sh:path acquisition:exposure_time ],
        [ sh:class acquisition:Detector ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 10 ;
            sh:path acquisition:detectors ],
        [ sh:datatype xsd:string ;
            sh:description "Cryogen used in cooling the instrument and sample, usually nitrogen" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 16 ;
            sh:path acquisition:cryogen ],
        [ sh:datatype xsd:string ;
            sh:description "Software used for instrument control," ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 9 ;
            sh:path acquisition:microscope_software ],
        [ sh:class types:QuantitySI ;
            sh:description "The total electron beam current hitting the viewing screen, in nA." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path acquisition:screen_current ],
        [ sh:datatype xsd:string ;
            sh:description "Specifies which Acquisition subschema/class is in use." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path acquisition:technique ] ;
    sh:targetClass acquisition:Acquisition .

