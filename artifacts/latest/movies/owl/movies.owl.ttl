<https://w3id.org/oscem-schemas/latest/custom_typeswidth> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "width" ;
    rdfs:range linkml:Integer ;
    skos:definition "The width of a given item - unit depends on item" ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/custom_types> .

<https://w3id.org/oscem-schemas/latest/custom_typesx_max> a owl:ObjectProperty,
