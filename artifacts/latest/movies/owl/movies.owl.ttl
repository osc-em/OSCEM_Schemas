<https://w3id.org/oscem-schemas/latest/custom_typesmaximal> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "maximal" ;
    rdfs:range <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
    skos:definition "Maximal value of a given dataset property" ;
