<https://w3id.org/oscem-schemas/latest/custom_typesy_min> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "y_min" ;
    rdfs:range <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
    skos:definition "minimum y" ;
