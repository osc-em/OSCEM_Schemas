<https://w3id.org/oscem-schemas/latest/custom_typesminimal> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "minimal" ;
    rdfs:range <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
    skos:definition "Minimal value of a given dataset property" ;
