    skos:inScheme <https://w3id.org/oscem-schemas/latest/sample-env> .

sample_env:organelles a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "organelles" ;
    rdfs:range linkml:String ;
    skos:definition "What organelles; if any; are present?" ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/sample-env> .

sample_env:organism a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "organism" ;
    rdfs:range linkml:String ;
    skos:definition "the organism(s) present in your sample, if not perfectly defined try to asses as close as possible." ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/sample-env> .

<https://w3id.org/oscem-schemas/latest/custom_typesdescriptor_name> a owl:ObjectProperty,
