@prefix linkml: <https://w3id.org/linkml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sample_env: <https://w3id.org/oscem-schemas/latest/environmental_sample> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://w3id.org/oscem-schemas/latest/custom_typesBoundingBox2D> a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "BoundingBox2D" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesy_max> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesy_max> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesy_min> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesy_min> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesx_max> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesx_max> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesy_min> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesx_min> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesy_max> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesx_max> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesx_min> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesx_min> ] ;
    skos:definition "an axis-aligned 2D bounding box (float units)" ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/custom_types> .

<https://w3id.org/oscem-schemas/latest/custom_typesImageSize> a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "ImageSize" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom linkml:Integer ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesheight> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesheight> ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Integer ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typeswidth> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typeswidth> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typeswidth> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesheight> ] ;
    skos:definition "size of a 2D image (in integer units)" ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/custom_types> .

<https://w3id.org/oscem-schemas/latest/custom_typesSeries> a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Series" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesincrement> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesincrement> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesincrement> ],
        <https://w3id.org/oscem-schemas/latest/custom_typesRange> ;
    skos:definition "A series of numbers constructed from min, max, and increment" ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/custom_types> .

<https://w3id.org/oscem-schemas/latest/custom_typesdescription> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "description" ;
    rdfs:range linkml:String ;
    skos:definition "The description of the item" ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/custom_types> .

<https://w3id.org/oscem-schemas/latest/custom_typesdescriptors> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "descriptors" ;
    rdfs:range <https://w3id.org/oscem-schemas/latest/custom_typesDescriptors> ;
    skos:definition "List of custom descriptors for user-defined key-value pairs describing how movies were obtained or any related information" ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/custom_types> .

<https://w3id.org/oscem-schemas/latest/custom_typesmanufacturer> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "manufacturer" ;
    rdfs:range linkml:String ;
    skos:definition "The name of the manufacturer" ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/custom_types> .

<https://w3id.org/oscem-schemas/latest/custom_typesmodel> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "model" ;
    rdfs:range linkml:String ;
    skos:definition "The model of the item" ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/custom_types> .

<https://w3id.org/oscem-schemas/latest/custom_typesname> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "name" ;
    rdfs:range linkml:String ;
    skos:definition "The name of the item" ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/custom_types> .

sample_env:SampleEnv a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "SampleEnv" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty sample_env:specimen_env ],
        [ a owl:Restriction ;
            owl:allValuesFrom sample_env:Freezing ;
            owl:onProperty sample_env:freezing ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sample_env:freezing ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sample_env:thinning ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sample_env:tomogram_features ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:thinning ],
        [ a owl:Restriction ;
            owl:allValuesFrom sample_env:TomogramFeatures ;
            owl:onProperty sample_env:tomogram_features ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:specimen_env ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:tomogram_features ],
        [ a owl:Restriction ;
            owl:allValuesFrom sample_env:SpecimenEnv ;
            owl:onProperty sample_env:specimen_env ],
        [ a owl:Restriction ;
            owl:allValuesFrom sample_env:Thinning ;
            owl:onProperty sample_env:thinning ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:freezing ] ;
    skos:definition "Unifying class to describe the full sample." ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/sample-env> .

<https://w3id.org/oscem-schemas/latest/custom_typesDescriptor> a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Descriptor" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/oscem-schemas/latest/custom_typesAny> ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesdescriptor_thing> ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesdescriptor_name> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesdescriptor_thing> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesdescriptor_thing> ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesdescriptor_name> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesdescriptor_name> ] ;
    skos:definition "List of custom descriptors for user-defined key-value pairs describing how micrographs were obtained or any related information" ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/custom_types> .

<https://w3id.org/oscem-schemas/latest/custom_typesDescriptors> a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Descriptors" ;
    rdfs:subClassOf <https://w3id.org/oscem-schemas/latest/custom_typesDescriptor> ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/custom_types> .

<https://w3id.org/oscem-schemas/latest/custom_typesQuantityValue> a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "QuantityValue" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesunit> ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesunit> ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesunit> ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesvalue> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesvalue> ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Float ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesvalue> ] ;
    skos:definition "if a value has a unit, it should be given as a unit value pair." ;
    skos:exactMatch <qudt:quantityValue> ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/custom_types> .

<https://w3id.org/oscem-schemas/latest/custom_typesRange> a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Range" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesminimal> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesmaximal> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesminimal> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesmaximal> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesmaximal> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesminimal> ] ;
    skos:definition "A range constructed from min and max" ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/custom_types> .

<https://w3id.org/oscem-schemas/latest/custom_typesAny> a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Any" ;
    skos:definition "Any type, used as the base for type-narrowing." ;
    skos:exactMatch linkml:Any ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/custom_types> .

sample_env:Freezing a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Freezing" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:atmosphere ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sample_env:blotting ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty sample_env:cryogen_sample_env ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:details ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sample_env:cryogen_sample_env ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sample_env:humidity_env ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
            owl:onProperty sample_env:temperature_env ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:blotting ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:humidity_env ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:method ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sample_env:temperature_env ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:cryogen_sample_env ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty sample_env:details ],
        [ a owl:Restriction ;
            owl:allValuesFrom sample_env:FreezingMethodEnum ;
            owl:onProperty sample_env:method ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:temperature_env ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty sample_env:atmosphere ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sample_env:method ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Boolean ;
            owl:onProperty sample_env:blotting ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
            owl:onProperty sample_env:humidity_env ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sample_env:atmosphere ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sample_env:details ] ;
    skos:definition "how the sample was frozen." ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/sample-env> .

<https://w3id.org/oscem-schemas/latest/environmental_sampleFreezingMethodEnum#high_pressure_freezing> a owl:Class,
        sample_env:FreezingMethodEnum ;
    rdfs:label "high_pressure_freezing" ;
    rdfs:subClassOf sample_env:FreezingMethodEnum ;
    skos:definition "Usually used for larger organic complexes, such as organoids or even tissue samples." .

<https://w3id.org/oscem-schemas/latest/environmental_sampleFreezingMethodEnum#plunge_freezing> a owl:Class,
        sample_env:FreezingMethodEnum ;
    rdfs:label "plunge_freezing" ;
    rdfs:subClassOf sample_env:FreezingMethodEnum ;
    skos:definition "Typically used for smaller and thinner samples in both SPA and tomography." .

sample_env:SpecimenEnv a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "SpecimenEnv" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty sample_env:organism ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty sample_env:organism ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sample_env:handling ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sample_env:tissue ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:tissue ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:location ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:source_env ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:handling ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty sample_env:handling ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty sample_env:source_env ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Date ;
            owl:onProperty sample_env:collection_date ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty sample_env:location ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:collection_date ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sample_env:collection_date ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sample_env:location ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty sample_env:tissue ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sample_env:source_env ] ;
    skos:definition "base information on the acquisition and treatment of the sample itself." ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/sample-env> .

sample_env:Thinning a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Thinning" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:method_thin ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sample_env:method_thin ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:instrument_thin ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sample_env:instrument_thin ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:ion_source ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty sample_env:ion_source ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:target_thickness ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Boolean ;
            owl:onProperty sample_env:lift_out ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sample_env:ion_source ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty sample_env:method_thin ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sample_env:target_thickness ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sample_env:lift_out ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty sample_env:instrument_thin ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:lift_out ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
            owl:onProperty sample_env:target_thickness ] ;
    skos:definition "how the frozen sample was thinned by i.e. FiB-milling" ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/sample-env> .

sample_env:TomogramFeatures a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "TomogramFeatures" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sample_env:organelles ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sample_env:cellular_features ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty sample_env:main_target ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty sample_env:cellular_features ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:details_tomo ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty sample_env:main_target ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:cellular_features ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty sample_env:main_target ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty sample_env:organelles ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
