@prefix custom_types: <https://w3id.org/oscem-schemas/latest/custom_types> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

custom_types:BoundingBox2D a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "BoundingBox2D" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty custom_types:y_max ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty custom_types:x_max ],
        [ a owl:Restriction ;
            owl:allValuesFrom custom_types:QuantitySI ;
            owl:onProperty custom_types:x_max ],
        [ a owl:Restriction ;
            owl:allValuesFrom custom_types:QuantitySI ;
            owl:onProperty custom_types:y_min ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty custom_types:y_min ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty custom_types:x_max ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty custom_types:y_max ],
        [ a owl:Restriction ;
            owl:allValuesFrom custom_types:QuantitySI ;
            owl:onProperty custom_types:y_max ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty custom_types:y_min ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty custom_types:x_min ],
        [ a owl:Restriction ;
            owl:allValuesFrom custom_types:QuantitySI ;
            owl:onProperty custom_types:x_min ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty custom_types:x_min ] ;
    skos:definition "an axis-aligned 2D bounding box (float units)" ;
    skos:inScheme custom_types: .

custom_types:ImageSize a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "ImageSize" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom linkml:Integer ;
            owl:onProperty custom_types:width ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty custom_types:width ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Integer ;
            owl:onProperty custom_types:height ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty custom_types:height ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty custom_types:width ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty custom_types:height ] ;
    skos:definition "size of a 2D image (in integer units)" ;
    skos:inScheme custom_types: .

custom_types:Series a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Series" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty custom_types:increment ],
        [ a owl:Restriction ;
            owl:allValuesFrom custom_types:QuantitySI ;
            owl:onProperty custom_types:increment ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty custom_types:increment ],
        custom_types:Range ;
    skos:definition "A series of numbers constructed from min, max, and increment" ;
    skos:inScheme custom_types: .

custom_types:description a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "description" ;
    rdfs:range linkml:String ;
    skos:definition "The description of the item" ;
    skos:inScheme custom_types: .

custom_types:descriptors a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "descriptors" ;
    rdfs:range custom_types:Descriptors ;
    skos:definition "List of custom descriptors for user-defined key-value pairs describing how movies were obtained or any related information" ;
    skos:inScheme custom_types: .

custom_types:manufacturer a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "manufacturer" ;
    rdfs:range linkml:String ;
    skos:definition "The name of the manufacturer" ;
    skos:inScheme custom_types: .

custom_types:model a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "model" ;
    rdfs:range linkml:String ;
    skos:definition "The model of the item" ;
    skos:inScheme custom_types: .

custom_types:name a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "name" ;
    rdfs:range linkml:String ;
    skos:definition "The name of the item" ;
    skos:inScheme custom_types: .

custom_types:Descriptor a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Descriptor" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty custom_types:descriptor_thing ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty custom_types:descriptor_thing ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty custom_types:descriptor_name ],
        [ a owl:Restriction ;
            owl:allValuesFrom custom_types:Any ;
            owl:onProperty custom_types:descriptor_thing ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty custom_types:descriptor_name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty custom_types:descriptor_name ] ;
    skos:definition "List of custom descriptors for user-defined key-value pairs describing how micrographs were obtained or any related information" ;
    skos:inScheme custom_types: .

custom_types:Descriptors a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Descriptors" ;
    rdfs:subClassOf custom_types:Descriptor ;
    skos:inScheme custom_types: .

custom_types:QuantityValue a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "QuantityValue" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty custom_types:unit ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Float ;
            owl:onProperty custom_types:value ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty custom_types:value ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty custom_types:unit ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty custom_types:unit ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty custom_types:value ] ;
    skos:definition "if a value has a unit, it should be given as a unit value pair." ;
    skos:exactMatch <qudt:quantityValue> ;
    skos:inScheme custom_types: .

custom_types:Range a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Range" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty custom_types:minimal ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty custom_types:maximal ],
        [ a owl:Restriction ;
            owl:allValuesFrom custom_types:QuantitySI ;
            owl:onProperty custom_types:maximal ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty custom_types:minimal ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty custom_types:maximal ],
        [ a owl:Restriction ;
            owl:allValuesFrom custom_types:QuantitySI ;
            owl:onProperty custom_types:minimal ] ;
    skos:definition "A range constructed from min and max" ;
    skos:inScheme custom_types: .

custom_types:Any a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Any" ;
    skos:definition "Any type, used as the base for type-narrowing." ;
    skos:exactMatch linkml:Any ;
    skos:inScheme custom_types: .

custom_types:descriptor_name a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "descriptor_name" ;
    rdfs:range linkml:String ;
    skos:definition "Name defining the descriptor" ;
    skos:inScheme custom_types: .

custom_types:descriptor_thing a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "descriptor_thing" ;
    rdfs:range custom_types:Any ;
    skos:definition "Description of the descriptor" ;
    skos:inScheme custom_types: .

custom_types:height a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "height" ;
    rdfs:range linkml:Integer ;
    skos:definition "The height of a given item - unit depends on item" ;
    skos:inScheme custom_types: .

custom_types:increment a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "increment" ;
    rdfs:range custom_types:QuantitySI ;
    skos:definition "Increment between elements of a series" ;
    skos:inScheme custom_types: .

custom_types:maximal a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "maximal" ;
    rdfs:range custom_types:QuantitySI ;
    skos:definition "Maximal value of a given dataset property" ;
    skos:inScheme custom_types: .

custom_types:minimal a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "minimal" ;
    rdfs:range custom_types:QuantitySI ;
    skos:definition "Minimal value of a given dataset property" ;
    skos:inScheme custom_types: .

custom_types:unit a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "unit" ;
    rdfs:range linkml:String ;
    skos:definition "the unit of a given value" ;
    skos:inScheme custom_types: .

custom_types:unitSI a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "unitSI" ;
    rdfs:range linkml:String ;
    skos:definition "the SI unit attached to a si value" ;
    skos:inScheme custom_types: .

custom_types:value a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "value" ;
    rdfs:range linkml:Float ;
    skos:definition "the value of a field with a unit" ;
    skos:inScheme custom_types: .

custom_types:valueSI a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "valueSI" ;
    rdfs:range linkml:Float ;
    skos:definition "value of a given field in respect to its SI unit" ;
    skos:inScheme custom_types: .

custom_types:width a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "width" ;
    rdfs:range linkml:Integer ;
    skos:definition "The width of a given item - unit depends on item" ;
    skos:inScheme custom_types: .

custom_types:x_max a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "x_max" ;
    rdfs:range custom_types:QuantitySI ;
    skos:definition "maximum x" ;
    skos:inScheme custom_types: .

custom_types:x_min a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "x_min" ;
    rdfs:range custom_types:QuantitySI ;
    skos:definition "minimum x" ;
    skos:inScheme custom_types: .

custom_types:y_max a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "y_max" ;
    rdfs:range custom_types:QuantitySI ;
    skos:definition "maximum y" ;
    skos:inScheme custom_types: .

custom_types:y_min a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "y_min" ;
    rdfs:range custom_types:QuantitySI ;
    skos:definition "minimum y" ;
    skos:inScheme custom_types: .

custom_types:QuantitySI a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "QuantitySI" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty custom_types:valueSI ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty custom_types:unitSI ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty custom_types:unitSI ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty custom_types:valueSI ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty custom_types:unitSI ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Float ;
            owl:onProperty custom_types:valueSI ],
        custom_types:QuantityValue ;
    skos:definition "unit value extended to have two additional fields si_value and si_unit" ;
    skos:inScheme custom_types: .

custom_types: a owl:Ontology ;
    rdfs:label "CustomTypes" ;
    skos:definition "Mixins for basic types shared among OSC-EM schemas." .

