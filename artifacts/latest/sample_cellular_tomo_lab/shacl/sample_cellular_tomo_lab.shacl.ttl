@prefix cellular_tomo: <https://w3id.org/oscem-schemas/latest/cellular_tomo> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

cellular_tomo:SampleCell a sh:NodeShape ;
    sh:closed true ;
    sh:description "Unifying class to describe the full sample with growth conditions" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class <https://w3id.org/oscem-schemas/latest/environmental_sampleTomogramFeatures> ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_sampletomogram_features> ],
        [ sh:class <https://w3id.org/oscem-schemas/latest/environmental_sampleThinning> ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_samplethinning> ],
        [ sh:class cellular_tomo:GrowthCondition ;
            sh:description "How the specimen was grown" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path cellular_tomo:growth_condition ],
        [ sh:class <https://w3id.org/oscem-schemas/latest/environmental_sampleSpecimenEnv> ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_samplespecimen_env> ],
        [ sh:class <https://w3id.org/oscem-schemas/latest/environmental_sampleFreezing> ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_samplefreezing> ] ;
    sh:targetClass cellular_tomo:SampleCell .

<https://w3id.org/oscem-schemas/latest/custom_typesBoundingBox2D> a sh:NodeShape ;
    sh:closed true ;
    sh:description "an axis-aligned 2D bounding box (float units)" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
            sh:description "maximum x" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path <https://w3id.org/oscem-schemas/latest/custom_typesx_max> ],
        [ sh:class <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
            sh:description "maximum y" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path <https://w3id.org/oscem-schemas/latest/custom_typesy_max> ],
        [ sh:class <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
            sh:description "minimum y" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path <https://w3id.org/oscem-schemas/latest/custom_typesy_min> ],
        [ sh:class <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
            sh:description "minimum x" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path <https://w3id.org/oscem-schemas/latest/custom_typesx_min> ] ;
    sh:targetClass <https://w3id.org/oscem-schemas/latest/custom_typesBoundingBox2D> .

<https://w3id.org/oscem-schemas/latest/custom_typesDescriptor> a sh:NodeShape ;
    sh:closed false ;
    sh:description "List of custom descriptors for user-defined key-value pairs describing how micrographs were obtained or any related information" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Name defining the descriptor" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path <https://w3id.org/oscem-schemas/latest/custom_typesdescriptor_name> ],
        [ sh:class linkml:Any ;
            sh:description "Description of the descriptor" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path <https://w3id.org/oscem-schemas/latest/custom_typesdescriptor_thing> ] ;
    sh:targetClass <https://w3id.org/oscem-schemas/latest/custom_typesDescriptor> .

<https://w3id.org/oscem-schemas/latest/custom_typesDescriptors> a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Name defining the descriptor" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path <https://w3id.org/oscem-schemas/latest/custom_typesdescriptor_name> ],
        [ sh:class linkml:Any ;
            sh:description "Description of the descriptor" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path <https://w3id.org/oscem-schemas/latest/custom_typesdescriptor_thing> ] ;
    sh:targetClass <https://w3id.org/oscem-schemas/latest/custom_typesDescriptors> .

<https://w3id.org/oscem-schemas/latest/custom_typesImageSize> a sh:NodeShape ;
    sh:closed true ;
    sh:description "size of a 2D image (in integer units)" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:integer ;
            sh:description "The width of a given item - unit depends on item" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path <https://w3id.org/oscem-schemas/latest/custom_typeswidth> ],
        [ sh:datatype xsd:integer ;
            sh:description "The height of a given item - unit depends on item" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path <https://w3id.org/oscem-schemas/latest/custom_typesheight> ] ;
    sh:targetClass <https://w3id.org/oscem-schemas/latest/custom_typesImageSize> .

<https://w3id.org/oscem-schemas/latest/custom_typesRange> a sh:NodeShape ;
    sh:closed true ;
    sh:description "A range constructed from min and max" ;
    sh:ignoredProperties ( rdf:type <https://w3id.org/oscem-schemas/latest/custom_typesincrement> ) ;
    sh:property [ sh:class <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
            sh:description "Maximal value of a given dataset property" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path <https://w3id.org/oscem-schemas/latest/custom_typesmaximal> ],
        [ sh:class <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
            sh:description "Minimal value of a given dataset property" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path <https://w3id.org/oscem-schemas/latest/custom_typesminimal> ] ;
    sh:targetClass <https://w3id.org/oscem-schemas/latest/custom_typesRange> .

<https://w3id.org/oscem-schemas/latest/custom_typesSeries> a sh:NodeShape ;
    sh:closed true ;
    sh:description "A series of numbers constructed from min, max, and increment" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
            sh:description "Maximal value of a given dataset property" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path <https://w3id.org/oscem-schemas/latest/custom_typesmaximal> ],
        [ sh:class <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
            sh:description "Minimal value of a given dataset property" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path <https://w3id.org/oscem-schemas/latest/custom_typesminimal> ],
        [ sh:class <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
            sh:description "Increment between elements of a series" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path <https://w3id.org/oscem-schemas/latest/custom_typesincrement> ] ;
    sh:targetClass <https://w3id.org/oscem-schemas/latest/custom_typesSeries> .

<https://w3id.org/oscem-schemas/latest/environmental_sampleSampleEnv> a sh:NodeShape ;
    sh:closed true ;
    sh:description "Unifying class to describe the full sample." ;
    sh:ignoredProperties ( cellular_tomo:growth_condition rdf:type ) ;
    sh:property [ sh:class <https://w3id.org/oscem-schemas/latest/environmental_sampleTomogramFeatures> ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_sampletomogram_features> ],
        [ sh:class <https://w3id.org/oscem-schemas/latest/environmental_sampleFreezing> ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_samplefreezing> ],
        [ sh:class <https://w3id.org/oscem-schemas/latest/environmental_sampleThinning> ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 2 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_samplethinning> ],
        [ sh:class <https://w3id.org/oscem-schemas/latest/environmental_sampleSpecimenEnv> ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 0 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_samplespecimen_env> ] ;
    sh:targetClass <https://w3id.org/oscem-schemas/latest/environmental_sampleSampleEnv> .

<qudt:quantityValue> a sh:NodeShape ;
    sh:closed true ;
    sh:description "if a value has a unit, it should be given as a unit value pair." ;
    sh:ignoredProperties ( <https://w3id.org/oscem-schemas/latest/custom_typesvalueSI> <https://w3id.org/oscem-schemas/latest/custom_typesunitSI> rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "the unit of a given value" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path <qudt:hasUnit> ],
        [ sh:datatype xsd:float ;
            sh:description "the value of a field with a unit" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path <qudt:hasQuantityKind> ] ;
    sh:targetClass <qudt:quantityValue> .

cellular_tomo:GrowthCondition a sh:NodeShape ;
    sh:closed true ;
    sh:description "How the cells were grown" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "What growth media was used" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path cellular_tomo:media ],
        [ sh:datatype xsd:string ;
            sh:description "In/on what kind of surface/container the cells were grown; i.e. directly on a grid" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path cellular_tomo:growth_location ],
        [ sh:datatype xsd:string ;
            sh:description "What was the targeted cell cycle state, if any" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path cellular_tomo:cell_cycle ],
        [ sh:datatype xsd:string ;
            sh:description "What was the atmosphere the sample was in, elaborate on any special gases present etc." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path cellular_tomo:atmosphere_growth ],
        [ sh:class <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
            sh:description "Temperature of the sample; in K." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 5 ;
            sh:path cellular_tomo:temperature_growth ],
        [ sh:datatype xsd:string ;
            sh:description "Were there any special treatment conditions; such as addition of certain chemicals etc" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path cellular_tomo:treatment ] ;
    sh:targetClass cellular_tomo:GrowthCondition .

linkml:Any a sh:NodeShape ;
    sh:closed true ;
    sh:description "Any type, used as the base for type-narrowing." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:targetClass linkml:Any .

<https://w3id.org/oscem-schemas/latest/environmental_sampleFreezing> a sh:NodeShape ;
    sh:closed true ;
    sh:description "how the sample was frozen." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "freezing method - such as plunge freezing, high pressure freezing etc." ;
            sh:in ( "plunge_freezing" "high_pressure_freezing" ) ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_samplemethod> ],
        [ sh:datatype xsd:string ;
            sh:description "the cryogen used to freeze the sample, i.e. ethane" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_samplecryogen_sample_env> ],
        [ sh:datatype xsd:string ;
            sh:description "What was the atmosphere the sample was in right before freezing, elaborate on any special gases present etc." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_sampleatmosphere> ],
        [ sh:class <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
            sh:description "humidity of the atmosphere right before/ during freezing; in %." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_samplehumidity_env> ],
        [ sh:datatype xsd:string ;
            sh:description "Any further comments on the freezing process go here." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_sampledetails> ],
        [ sh:class <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
            sh:description "temperature of the sample right before freezing; in K." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 4 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_sampletemperature_env> ],
        [ sh:datatype xsd:boolean ;
            sh:description "whether blotting was performed." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_sampleblotting> ] ;
    sh:targetClass <https://w3id.org/oscem-schemas/latest/environmental_sampleFreezing> .

<https://w3id.org/oscem-schemas/latest/environmental_sampleSpecimenEnv> a sh:NodeShape ;
    sh:closed true ;
    sh:description "base information on the acquisition and treatment of the sample itself." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "the organism(s) present in your sample, if not perfectly defined try to asses as close as possible." ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_sampleorganism> ],
        [ sh:datatype xsd:string ;
            sh:description "What was done to the sample, please give an overview of relevant treatments." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_samplehandling> ],
        [ sh:datatype xsd:date ;
            sh:description "When the sample was collected" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_samplecollection_date> ],
        [ sh:datatype xsd:string ;
            sh:description "the geographical location of your source, optimally in a geographic coordinate system." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_samplelocation> ],
        [ sh:datatype xsd:string ;
            sh:description "if the sample is a tissue sample please indicate what type of tissue." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_sampletissue> ],
        [ sh:datatype xsd:string ;
            sh:description "where is this sample from? i.e. Hospital" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_samplesource_env> ] ;
    sh:targetClass <https://w3id.org/oscem-schemas/latest/environmental_sampleSpecimenEnv> .

<https://w3id.org/oscem-schemas/latest/environmental_sampleThinning> a sh:NodeShape ;
    sh:closed true ;
    sh:description "how the frozen sample was thinned by i.e. FiB-milling" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "Instrument used for thinning the sample" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_sampleinstrument_thin> ],
        [ sh:datatype xsd:string ;
            sh:description "The thinning method used, such as FIB milling." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_samplemethod_thin> ],
        [ sh:datatype xsd:boolean ;
            sh:description "whether a lift out was performed" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_samplelift_out> ],
        [ sh:datatype xsd:string ;
            sh:description "what ion source was used?" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_sampleion_source> ],
        [ sh:class <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> ;
            sh:description "What was the target thickness of the lamella; in nm." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 3 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_sampletarget_thickness> ] ;
    sh:targetClass <https://w3id.org/oscem-schemas/latest/environmental_sampleThinning> .

<https://w3id.org/oscem-schemas/latest/environmental_sampleTomogramFeatures> a sh:NodeShape ;
    sh:closed true ;
    sh:description "what was the target of the tomograms, and what area of a cell do they cover." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "What type of cellular features are present in your tomograms?" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_samplecellular_features> ],
        [ sh:datatype xsd:string ;
            sh:description "What was the main biological target of your research for this tomogram?" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_samplemain_target> ],
        [ sh:datatype xsd:string ;
            sh:description "If you have any further comments on your tomograms please leave them here" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_sampledetails_tomo> ],
        [ sh:datatype xsd:string ;
            sh:description "What organelles; if any; are present?" ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path <https://w3id.org/oscem-schemas/latest/environmental_sampleorganelles> ] ;
    sh:targetClass <https://w3id.org/oscem-schemas/latest/environmental_sampleTomogramFeatures> .

<https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> a sh:NodeShape ;
    sh:closed true ;
    sh:description "unit value extended to have two additional fields si_value and si_unit" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "the unit of a given value" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path <qudt:hasUnit> ],
        [ sh:datatype xsd:float ;
            sh:description "the value of a field with a unit" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 3 ;
            sh:path <qudt:hasQuantityKind> ],
        [ sh:datatype xsd:float ;
            sh:description "value of a given field in respect to its SI unit" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path <https://w3id.org/oscem-schemas/latest/custom_typesvalueSI> ],
        [ sh:datatype xsd:string ;
            sh:description "the SI unit attached to a si value" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path <https://w3id.org/oscem-schemas/latest/custom_typesunitSI> ] ;
    sh:targetClass <https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> .

