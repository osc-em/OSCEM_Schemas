organizational:Author a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Author" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty organizational:given_name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty organizational:role ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty organizational:family_name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty organizational:country ],
        [ a owl:Restriction ;
            owl:allValuesFrom owl:Thing ;
            owl:onProperty organizational:given_name ],
        [ a owl:Restriction ;
            owl:allValuesFrom owl:Thing ;
            owl:onProperty organizational:email ],
        [ a owl:Restriction ;
            owl:allValuesFrom owl:Thing ;
            owl:onProperty organizational:family_name ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty organizational:name_org ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty organizational:role ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty organizational:type_org ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty organizational:email ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty organizational:role ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty organizational:country ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty organizational:name_org ],
        [ a owl:Restriction ;
