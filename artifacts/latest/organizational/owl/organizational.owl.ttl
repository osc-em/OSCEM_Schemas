    skos:definition "The name of the item" ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/custom_types> .

organizational:Organizational a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Organizational" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom organizational:Author ;
            owl:onProperty organizational:authors ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty organizational:funder ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty organizational:authors ],
        [ a owl:Restriction ;
            owl:allValuesFrom organizational:Funder ;
            owl:onProperty organizational:funder ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty organizational:grants ],
        [ a owl:Restriction ;
            owl:allValuesFrom organizational:Grant ;
            owl:onProperty organizational:grants ] ;
    skos:definition "Overarching category for authors and grants" ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/organizational> .

organizational:person a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "person" ;
    rdfs:range organizational:Person ;
    skos:definition "person" ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/organizational> .

<https://w3id.org/oscem-schemas/latest/custom_typesDescriptor> a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Descriptor" ;
    rdfs:subClassOf [ a owl:Restriction ;
