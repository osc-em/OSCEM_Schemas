<https://w3id.org/oscem-schemas/latest/organizational/:OrganizationTypeEnum> a owl:Class,
        linkml:EnumDefinition ;
    rdfs:label "OrganizationTypeEnum" ;
    owl:unionOf ( <https://w3id.org/oscem-schemas/latest/organizational/:OrganizationTypeEnum#Academic> <https://w3id.org/oscem-schemas/latest/organizational/:OrganizationTypeEnum#Commercial> <https://w3id.org/oscem-schemas/latest/organizational/:OrganizationTypeEnum#Government> <https://w3id.org/oscem-schemas/latest/organizational/:OrganizationTypeEnum#Other> ) ;
    skos:definition "Allowed values for authors organizations." ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/organizational> ;
    linkml:permissible_values <https://w3id.org/oscem-schemas/latest/organizational/:OrganizationTypeEnum#Academic>,
        <https://w3id.org/oscem-schemas/latest/organizational/:OrganizationTypeEnum#Commercial>,
        <https://w3id.org/oscem-schemas/latest/organizational/:OrganizationTypeEnum#Government>,
        <https://w3id.org/oscem-schemas/latest/organizational/:OrganizationTypeEnum#Other> .

<https://w3id.org/oscem-schemas/latest/custom_typesQuantitySI> a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "QuantitySI" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesunitSI> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesvalueSI> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesvalueSI> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesunitSI> ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Float ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesvalueSI> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesunitSI> ],
        <https://w3id.org/oscem-schemas/latest/custom_typesQuantityValue> ;
    skos:definition "unit value extended to have two additional fields si_value and si_unit" ;
    skos:inScheme <https://w3id.org/oscem-schemas/latest/custom_types> .

