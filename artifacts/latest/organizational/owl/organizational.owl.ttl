            owl:allValuesFrom linkml:String ;
            owl:onProperty organizational:funder_name ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty organizational:country ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/oscem-schemas/latest/organizational/:OrganizationTypeEnum> ;
            owl:onProperty organizational:type_org ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty organizational:funder_name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty organizational:type_org ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty organizational:country ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty organizational:funder_name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty organizational:type_org ],
