        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty organizational:orcid ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty organizational:type_org ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty organizational:orcid ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty organizational:orcid ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty organizational:family_name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty organizational:name_org ],
        organizational:Person ;
