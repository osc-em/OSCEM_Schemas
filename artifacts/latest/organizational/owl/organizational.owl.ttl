        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesdescriptor_name> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://w3id.org/oscem-schemas/latest/custom_typesdescriptor_thing> ],
        [ a owl:Restriction ;
