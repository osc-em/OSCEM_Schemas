            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 21 ;
            sh:path <https://w3id.org/oscem-schemas/latest/acquisitionpixel_size> ],
        [ sh:datatype xsd:string ;
            sh:description "Type of cryogen used in the holder - if the holder is cooled seperately" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path <https://w3id.org/oscem-schemas/latest/acquisitionholder_cryogen> ],
        [ sh:class <https://w3id.org/oscem-schemas/latest/acquisitionEnergyFilter> ;
